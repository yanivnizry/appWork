{
  "name": "rn-clock",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "build:ios:dev": "npx expo run:ios --configuration Debug",
    "build:ios:release": "npx expo run:ios --configuration Release",
    "build:android:dev": "npx expo run:android --variant debug",
    "build:android:release": "npx expo run:android --variant release",
    "build:ios:eas": "eas build --platform ios",
    "build:android:eas": "eas build --platform android",
    "build:all:eas": "eas build --platform all"
  },
  "dependencies": {
    "@shopify/react-native-skia": "^2.4.6",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-linking": "^8.0.9",
    "expo-router": "^6.0.15",
    "expo-sqlite": "^16.0.9",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-mmkv": "^4.0.1",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
